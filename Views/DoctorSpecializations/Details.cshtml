@model DentalCenter.Models.DoctorSpecialization

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<main role="main" class="pb-3">
    <div class="container">

        <h1>Details</h1>

        <div>
            <h4>DoctorSpecialization</h4>
            <hr />
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Doctor)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Doctor.DoctorId)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Specialization)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Specialization.SpecializationId)
                </dd>
            </dl>
        </div>
        <div>
            <a asp-action="Edit" asp-route-id="@Model?.DoctorSpecializationId">Edit</a> |
            <a asp-action="Index">Back to List</a>
        </div>
    </div>
</main>

