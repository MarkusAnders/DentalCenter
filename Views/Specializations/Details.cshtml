@model DentalCenter.Models.Specialization

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<main role="main" class="pb-3">
    <div class="container">

        <h1>Details</h1>

        <div>
            <h4>Specialization</h4>
            <hr />
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.SpecializationName)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.SpecializationName)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.SpecializationEducation)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.SpecializationEducation)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.SpecializationWorkExp)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.SpecializationWorkExp)
                </dd>
            </dl>
        </div>
        <div>
            <a asp-action="Edit" asp-route-id="@Model?.SpecializationId">Edit</a> |
            <a asp-action="Index">Back to List</a>
        </div>

    </div>
</main>
